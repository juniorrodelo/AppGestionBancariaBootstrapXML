    <script type="text/javascript">
        function mostrar(id) {
            if (id == "corrientes") {
                $("#limitedecredito").show();
                $("#tasadeinteres").show();
                $("#talonario").hide();
                $("#tasadeinteresh").hide();
            }
            if (id == "cheques") {
                $("#limitedecredito").hide();
                $("#tasadeinteres").hide();
                $("#talonario").show();
                $("#tasadeinteresh").hide();
            }
            if (id == "ahorros") {
                $("#limitedecredito").hide();
                $("#tasadeinteres").hide();
                $("#talonario").hide();
                $("#tasadeinteresh").show();
            }
            if (id == "0") {
                $("#limitedecredito").hide();
                $("#tasadeinteres").hide();
                $("#talonario").hide();
                $("#tasadeinteresh").hide();
            }

        }
</script>
</head>
